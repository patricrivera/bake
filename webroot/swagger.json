    "paths": {
        "\/events": {
            "post": {
                "tags": [
                    "Events"
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application\/json": {
                            "schema": {
                                "$ref": "#\/components\/schemas\/Event"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/EventsResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "\/events\/instances": {
            "get": {
                "tags": [
                    "Events"
                ],
                "parameters": [
                    {
                        "in": "query",
                        "name": "start",
                        "schema": {
                            "type": "string",
                            "format": "date-time",
                            "example": "2020-02-13 01:00"
                        }
                    },
                    {
                        "in": "query",
                        "name": "end",
                        "schema": {
                            "type": "string",
                            "format": "date-time",
                            "example": "2021-02-13 08:00"
                        }
                    },
                    {
                        "in": "query",
                        "name": "invitees",
                        "schema": {
                            "type": "string",
                            "example": "1,2,3"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "type": "string",
                                    "example": [
                                        {
                                            "foo": "bar"
                                        }
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Event": {
                "description": "",
                "type": "object",
                "required": [
                    "eventName",
                    "duration",
                    "startDateTime",
                    "frequency",
                    "invitees"
                ],
                "properties": {
                    "eventName": {
                        "type": "string",
                        "example": "event 1"
                    },
                    "duration": {
                        "type": "integer",
                        "example": 30
                    },
                    "startDateTime": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2021-02-13 01:00"
                    },
                    "endDateTime": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2021-02-13 07:00"
                    },
                    "frequency": {
                        "type": "string",
                        "enum": [
                            "Once-Off",
                            "Weekly",
                            "Monthly"
                        ]
                    },
                    "invitees": {
                        "type": "array",
                        "example": [
                            1,
                            2,
                            3,
                            4
                        ],
                        "items": {
                            "type": "integer"
                        }
                    }
                }
            },
            "EventsResponse": {
                "description": "",
                "allOf": [
                    {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "integer"
                            }
                        },
                        "required": [
                            "id"
                        ]
                    },
                    {
                        "$ref": "#\/components\/schemas\/Event"
                    }
                ]
            }
        }
    },
